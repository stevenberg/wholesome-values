{
    "name": "stevenberg/wholesome-values",
    "type": "library",
    "description": "Library for representing Whole Values and Exceptional Values",
    "keywords": [
        "whole-value"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stevenberg/wholesome-values/issues",
        "source": "https://github.com/stevenberg/wholesome-values"
    },
    "authors": [
        {
            "name": "Steven Berg",
            "email": "steven@stevenberg.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "StevenBerg\\WholesomeValues\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StevenBerg\\WholesomeValues\\Tests\\": "tests/"
        }
    }
}
